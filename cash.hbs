     /* ainmations  */ 
    @keyframes Quotation{
      0%{opacity: 0;
        fill:none;
        stroke-dashoffset: 40;}
      50%{opacity: 10;
        fill:none;
        stroke-dashoffset: 40;}
      50%{
        fill:rgba(255,255,255,0);}
      100%{opacity: 10;
        fill:#9BF5E9;
        stroke-dashoffset: 0;}
    }
    
    
