    /* JUMP */
    .h {
      cursor: default;
      position: relative;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      width: 100%;
      height: 100px;
      margin: auto;
      display: block;
      text-align: center;
    }
    
    .h span {
      position: relative;
      top: 20px;
      display: inline-block;
      -webkit-animation: bounce 2s ease infinite alternate;
      font-size: 30px;
      color: #fff;
    }
    
    .h span:nth-child(2) {
      -webkit-animation-delay: 0.1s;
    }
    
    .h span:nth-child(3) {
      -webkit-animation-delay: 0.2s;
    }
    
    .h span:nth-child(4) {
      -webkit-animation-delay: 0.3s;
    }
    
    .h span:nth-child(5) {
      -webkit-animation-delay: 0.4s;
    }
    
    .h span:nth-child(6) {
      -webkit-animation-delay: 0.5s;
    }
    
    .h span:nth-child(7) {
      -webkit-animation-delay: 0.6s;
    }
    
    .h span:nth-child(8) {
      -webkit-animation-delay: 0.2s;
    }
    
    .h span:nth-child(9) {
      -webkit-animation-delay: 0.3s;
    }
    
    .h span:nth-child(10) {
      -webkit-animation-delay: 0.4s;
    }
    
    .h span:nth-child(11) {
      -webkit-animation-delay: 0.5s;
    }
    
    .h span:nth-child(12) {
      -webkit-animation-delay: 0.6s;
    }
    
    .h span:nth-child(13) {
      -webkit-animation-delay: 0.7s;
    }
    
    .h span:nth-child(14) {
      -webkit-animation-delay: 0.8s;
    }
    
    /* ANIMATION */
